{
	"version": "0.2.0",
	"configurations": [
		{
			"justMyCode": false,
			"name": "launch_debug",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/fdf.out",
			"args": [
				"./bin/test_maps/mars.fdf"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "lldb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "make_debug",
		}
	]
}